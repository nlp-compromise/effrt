{
  "name": "efrt",
  "description": "neato compression of key-value data",
  "version": "2.3.1",
  "main": "./builds/efrt.cjs",
  "unpkg": "./builds/efrt.min.js",
  "module": "./builds/efrt.mjs",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./builds/efrt.cjs",
      "import": "./builds/efrt.mjs",
      "default": "./builds/efrt.cjs"
    },
    "./unpack": {
      "import": "./builds/efrt-unpack.mjs",
      "default": "./builds/efrt-unpack.min.js"
    }
  },
  "author": "Spencer Kelly <spencermountain@gmail.com> (http://spencermounta.in)",
  "repository": {
    "type": "git",
    "url": "git://github.com/spencermountain/efrt.git"
  },
  "scripts": {
    "build": "node ./scripts/version.js && rollup -c ",
    "test": "tape-es \"./tests/*.test.js\" | tap-dancer",
    "testb": "TESTENV=prod tape-es \"./tests/*.test.js\" | tap-dancer",
    "watch": "amble ./scratch"
  },
  "files": [
    "builds/"
  ],
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 100
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/preset-env": "7.14.7",
    "amble": "1.3.0",
    "rollup": "2.53.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "7.0.2",
    "tap-dancer": "0.3.4",
    "tape": "^5.2.2",
    "tape-es": "^1.2.15"
  },
  "license": "MIT"
}
